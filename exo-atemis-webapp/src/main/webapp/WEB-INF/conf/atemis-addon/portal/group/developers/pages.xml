<?xml version="1.0" encoding="UTF-8"?>
<page-set xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_3 http://www.gatein.org/xml/ns/gatein_objects_1_3">
   <page>
      <name>ide</name>
      <title>eXo IDE</title>
      <access-permissions>*:/developers</access-permissions>
      <edit-permission>*:/platform/administrators</edit-permission>
      <show-max-window>false</show-max-window>
      <container id="eXo-IDE-container" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
         <access-permissions>*:/developers</access-permissions>
         <portlet-application>
            <portlet>
               <application-ref>dashboard</application-ref>
               <portlet-ref>GadgetPortlet</portlet-ref>
               <preferences>
                  <preference>
                     <name>fillUpFreeSpace</name>
                     <value>true</value>
                     <read-only>false</read-only>
                  </preference>
                  <preference>
                     <name>url</name>
                     <value>local://IDE</value>
                     <read-only>false</read-only>
                  </preference>
               </preferences>
            </portlet>
            <title>eXo IDE</title>
            <access-permissions>*:/developers</access-permissions>
            <show-info-bar>false</show-info-bar>
            <show-application-state>false</show-application-state>
            <show-application-mode>true</show-application-mode>
         </portlet-application>
      </container>
   </page>
</page-set>