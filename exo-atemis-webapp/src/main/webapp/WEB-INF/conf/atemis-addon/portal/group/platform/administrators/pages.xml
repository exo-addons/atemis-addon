   <page>
      <name>monitoring</name>
      <title>Monitoring</title>
      <access-permissions>manager:/platform/administrators</access-permissions>
      <edit-permission>manager:/platform/administrators</edit-permission>
      <show-max-window>false</show-max-window>
      <container id="Monitoringbody" template="system:/groovy/portal/webui/container/UITableColumnContainer.gtmpl">
         <access-permissions>manager:/platform/administrators</access-permissions>
         <container id="MonitoringLeft" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
            <access-permissions>manager:/platform/administrators</access-permissions>
            <portlet-application>
               <portlet>
                  <application-ref>dashboard</application-ref>
                  <portlet-ref>GadgetPortlet</portlet-ref>
                  <preferences>
                     <preference>
                        <name>url</name>
                        <value>local://MemoryCharts</value>
                        <read-only>false</read-only>
                     </preference>
                  </preferences>
               </portlet>
               <title>Gadget Portlet</title>
               <access-permissions>manager:/platform/administrators</access-permissions>
               <show-info-bar>false</show-info-bar>
               <show-application-state>true</show-application-state>
               <show-application-mode>true</show-application-mode>
            </portlet-application>
         </container>
         <container id="MonitoringMiddle" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
            <access-permissions>manager:/platform/administrators</access-permissions>
            <portlet-application>
               <portlet>
                  <application-ref>dashboard</application-ref>
                  <portlet-ref>GadgetPortlet</portlet-ref>
                  <preferences>
                     <preference>
                        <name>url</name>
                        <value>local://CacheLevels</value>
                        <read-only>false</read-only>
                     </preference>
                  </preferences>
               </portlet>
               <title>Gadget Portlet</title>
               <access-permissions>manager:/platform/administrators</access-permissions>
               <show-info-bar>false</show-info-bar>
               <show-application-state>true</show-application-state>
               <show-application-mode>true</show-application-mode>
            </portlet-application>
         </container>
         <container id="MonitoringRight" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
            <access-permissions>manager:/platform/administrators</access-permissions>
            <portlet-application>
               <portlet>
                  <application-ref>dashboard</application-ref>
                  <portlet-ref>GadgetPortlet</portlet-ref>
                  <preferences>
                     <preference>
                        <name>url</name>
                        <value>local://ApplicationsStatistics</value>
                        <read-only>false</read-only>
                     </preference>
                  </preferences>
               </portlet>
               <title>Gadget Portlet</title>
               <access-permissions>manager:/platform/administrators</access-permissions>
               <show-info-bar>false</show-info-bar>
               <show-application-state>true</show-application-state>
               <show-application-mode>true</show-application-mode>
            </portlet-application>
         </container>
      </container>
   </page>
